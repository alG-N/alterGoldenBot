    "@sentry/node": "^10.38.0",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.19.2",
    "dotenv": "^16.5.0",
    "ffmpeg-static": "^5.2.0",
    "graphql-request": "^7.1.2",
    "ioredis": "^5.9.2",
    "knex": "^3.1.0",
    "lavalink-client": "^2.7.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.12.0",
    "shoukaku": "^4.1.0",
    "tmp": "^0.2.5",
    "uuid": "^11.1.0",
    "youtubei.js": "^15.1.1",
